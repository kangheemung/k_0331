<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%=form_for "session", url: sessions_create_path do |f|%>
    <p><%= f.label:email %></p>
    <p><%= f.text_field :email, :placeholder => "email" %></p>
    <p><%= f.label :password %></p>
    <p><%= f.password_field :password %></p>
    <p><%= f.label :password_confirmation %></p>
    <p><%= f.password_field  :password_confirmation %></p>
    <p><%= f.label :remember_me, class: "checkbox inline" do %>
    <p><%= f.check_box :remember_me %>
      <span>Remember me on this computer</span>
    <% end %>
    <%=f.submit%>
<% end %>
  <p>New user? <%= link_to "Sign up now!",  users_new_path %></p>